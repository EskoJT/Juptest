    "def wave (string):\n",
    "    list = []\n",
    "    for s in range(0, len(string)):\n",
    "        if string[s].isalpha():\n",
    "            list.append(string[:s]+(string[s].upper())+string[s+1:])\n",
    "    return list"
   ]
